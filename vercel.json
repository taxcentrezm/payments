{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node"}
  ],
  "routes": [
    { "src": "/initiate-card-payment", "dest": "/api/initiate-card-payment.js"},
    { "src": "/check-payment-status", "dest": "/api/check-payment-status.js"},
    { "src": "/payment-callback", "dest": "/api/payment-callback.js"}
  ]
}
